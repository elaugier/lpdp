{
    "backendHostname": "lpdpbackend.local",
    "backendPort": "1204",
    "cockroach": {
        "lnx": {
            "cockroachArgs": "start --certs-dir=cockroach/certs --advertise-host=localhost",
            "cockroachPath": "cockroach/cockroach"
        },
        "mac": {
            "cockroachArgs": "start --certs-dir=cockroach/certs --advertise-host=localhost",
            "cockroachPath": "cockroach/cockroach"
        },
        "win": {
            "cockroachArgs": "/c \"cockroach\\cockroach.exe start --certs-dir=cockroach\\certs --advertise-host=localhost\"",
            "cockroachPath": "c:\\windows\\system32\\cmd.exe"
        }
    },
    "database": {
        "postgres": "postgresql://root@localhost:26257?sslmode=require&sslrootcert=cockroach\\certs\\ca.crt&sslcert=cockroach\\certs\\client.root.crt&sslkey=cockroach\\certs\\client.root.key"
    },
    "frontendHostname": "lpdpfrontend.local",
    "frontendPort": "1203",
    "gospell": {
        "aff": "hunspell/fr-toutesvariantes.aff",
        "dic": "hunspell/fr-toutesvariantes.dic"
    },
    "logFolder": "$TEMP"
}